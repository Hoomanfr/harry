{
  "engine": {
    "type": "redis",
    "redis": {
      "address": [
        "redis1:6379",
        "redis2:6379"
      ]
    }
  },
  "client": {
    "token": {
      "hmac_secret_key": "my_secret"
    },
    "allowed_origins": [
      "*"
    ]
  },
  "http_api": {
    "key": "my_api_key"
  },
  "admin": {
    "password": "password",
    "secret": "secret",
    "enabled": true
  },
  "channel": {
    "without_namespace": {
      "allow_subscribe_for_client": true,
      "allow_publish_for_client": true
    }
  }
}